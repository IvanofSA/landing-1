  section.about-company
    .container
      .header__title ПРЕИМУЩЕСТВА КОМПАНИИ STANDARTPLAST
      .line
      ul.skills__list
        li.skills__item 
          a(href="#").skills__link.skills__link_ready
          .skills__text Готовые решения
        li.skills__item

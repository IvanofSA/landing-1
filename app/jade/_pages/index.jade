              li.slider__item.active
                img(src="app/img/materials/materials1.jpg" alt="materials" class="slider__pic")
              li.slider__item 
                img(src="app/img/materials/materials2.jpg" alt="materials" class="slider__pic")
              li.slider__item 
                img(src="app/img/materials/materials3.jpg" alt="materials" class="slider__pic")
        li.tabs__item.tabs__item_third
